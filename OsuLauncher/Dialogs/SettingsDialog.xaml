<hc:Window x:Class="OsuLauncher.Dialogs.SettingsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:OsuLauncher.Dialogs"
        mc:Ignorable="d"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        WindowState="Normal"
        WindowStyle="ToolWindow"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="SettingsDialog" Height="548" Width="926">
    <DockPanel>
        <ListBox hc:BorderElement.CornerRadius="1" DockPanel.Dock="Left">
            <ListBoxItem Name="GeneralLbItem" Content="General"/>
            <ListBoxItem Name="GameLbItem" Content="Game Settings"/>
        </ListBox>
        <Frame Name="SettingsFrame" NavigationUIVisibility="Hidden" hc:BorderElement.CornerRadius="1" DockPanel.Dock="Right"/>
    </DockPanel>
</hc:Window>
