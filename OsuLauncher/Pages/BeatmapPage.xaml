<Page x:Class="OsuLauncher.Pages.BeatmapPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:OsuLauncher.Pages"
      mc:Ignorable="d"
      xmlns:hc="https://handyorg.github.io/handycontrol"
      MinHeight="450" MinWidth="800">
    <DockPanel>
        <hc:SearchBar hc:InfoElement.Placeholder="No Title" DockPanel.Dock="Top" hc:BorderElement.CornerRadius="2" Margin="3"/>
        <StackPanel Name="FilterBox" DockPanel.Dock="Left" Visibility="Collapsed" hc:BorderElement.CornerRadius="1" Margin="3">
            
        </StackPanel>
        <ListBox Name="BeatmapResultsBox" hc:BorderElement.CornerRadius="1" Margin="3">
            
        </ListBox>
    </DockPanel>
</Page>
