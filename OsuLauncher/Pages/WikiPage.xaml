<Page x:Class="OsuLauncher.Pages.WikiPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:OsuLauncher.Pages"
      mc:Ignorable="d"
      xmlns:hc="https://handyorg.github.io/handycontrol"
      MinHeight="450" MinWidth="800">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <hc:TextBox Text="https://osu.ppy.sh/wiki/en/Main_Page"/>
            <hc:SearchBar VerticalAlignment="Center"/>    
        </StackPanel>
        <FlowDocumentScrollViewer Name="WikiViewer"></FlowDocumentScrollViewer>
    </DockPanel>
</Page>
